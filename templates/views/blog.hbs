   <section class="breadcrumb_area">
        <img class="breadcrumb_shap" src="img/breadcrumb/banner_bg.png" alt="" data-pagespeed-url-hash="1219837120" onload="pagespeed.CriticalImages.checkImageForCriticality(this);">
        <div class="container">
            <div class="breadcrumb_content text-center">
                <h1 class="f_p f_700 f_size_50 w_color l_height50 mb_20">PasalSpace Blog </h1>
                <p class="f_400 w_color f_size_16 l_height26">Why I say old chap that is spiffing off his nut arse pear shaped plastered<br> Jeffrey bodge barney some dodgy.!!</p>
            </div>
        </div>
    </section>
    
    <section class="blog_area sec_pad">
        <div class="container">
            <div class="row" id="blog_masonry">            
                {{# each data.posts.results}}
                <div class="col-lg-4 col-sm-6">
                    <div class="blog_grid_item mb-30">
                        {{#if image}}
                        <div class="blog_img">
                            <img src="{{{cloudinaryUrl image width=370 height=320 crop='fit' }}}" alt="" data-pagespeed-url-hash="1117001457" onload="pagespeed.CriticalImages.checkImageForCriticality(this);">
                        </div>
                        {{/if}}
                        <div class="blog_content">
                            <div class="entry_post_info">
                                By: <a href="#">{{#if author.name.first}}by {{author.name.first}}{{/if}}</a>
                                <a href="#">{{ date publishedDate format='MMMM Do YYYY' }}</a>
                            </div>
                            <a href="{{postUrl slug}}"><h5 class="f_p f_size_20 f_500 t_color mb_20">{{{title}}}</h5></a>
                            <p class="f_400 mb-0">{{{content.brief}}}</p>
                        </div>
                    </div>
                </div>
                {{/each}}

                <ul class="col-lg-12 list-unstyled page-numbers shop_page_number text-left mt_30">
                    <li><span aria-current="page" class="page-numbers current">1</span></li>
                    <li><a class="page-numbers" href="#">2</a></li>
                    <li><a class="next page-numbers" href="#"><i class="ti-arrow-right"></i></a></li>
                </ul>
            </div>
        </div>
    </section>
   